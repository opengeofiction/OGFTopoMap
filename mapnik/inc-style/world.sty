<Style name="world">
	<Rule>
		<MaxScaleDenominator>250000000000</MaxScaleDenominator>
		<MinScaleDenominator>600000</MinScaleDenominator>
		<PolygonSymbolizer fill="white" />
	</Rule>
</Style>
<Style name="coast-poly">
	<Rule>
		&maxscale_zoom0;
		&minscale_zoom3;
		<PolygonSymbolizer fill="#117803" />  <!-- green to fill 0m of the relief -->
	</Rule>
	<Rule>
		&maxscale_zoom4;
		&minscale_zoom8;
		<PolygonSymbolizer fill="#117803" />  <!-- green to fill 0m of the relief -->
		<LineSymbolizer stroke="#117803" stroke-width="0.5" />
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom16;
		<PolygonSymbolizer fill="#f8f8e8" />  <!-- color taken from TK25 Sachsen -->
	</Rule>
</Style>
<Style name="coast-poly2">
	<Rule>
		<MaxScaleDenominator>1225681</MaxScaleDenominator>
		<MinScaleDenominator>9575</MinScaleDenominator>
		<PolygonSymbolizer fill="white" />
	</Rule>
	<Rule>
		<MaxScaleDenominator>600000</MaxScaleDenominator>
		<MinScaleDenominator>1225681</MinScaleDenominator>
		<PolygonSymbolizer fill="white" />
	</Rule>
</Style>
<Style name="builtup">
	<Rule>
		<MaxScaleDenominator>2500000</MaxScaleDenominator>
		<MinScaleDenominator>500000</MinScaleDenominator>
		<PolygonSymbolizer fill="#ddd" />
	</Rule>
</Style>
